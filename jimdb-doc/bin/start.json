{
  /**
   * * Application configuration section
   * */
  "apps": [
    {
      "name": "chubao-gitbook",
      "max_memory_restart": "300M",
      "script": "gitbook serve",
      "out_file": "/export/logs/out.log",
      "error_file": "/export/logs/err.log",
      "instances": 1,
      "env": {
        "NODE_ENV": "production"
      },
      "env_pre": {
        "NODE_ENV": "development"
      },
      "env_test": {
        "NODE_ENV": "test"
      }
    }
  ]
}